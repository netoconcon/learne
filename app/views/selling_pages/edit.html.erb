<h1>Editar pagina</h1>
<%= simple_form_for @selling_page do |f| %>
  <%= f.association :product, label: 'Nome do produto' %>
  <%= f.input :name, label: 'Nome da pagina' %>
  <%= f.input :url, label: 'Url' %>
  <%= f.input :description, label: 'Descricao' %>
  <%= f.button :submit, 'Editar pagina' %>
<% end %>